let xPos = 0; 
let yPos = 40; 

let xSpeed = 3;
let ySpeed = 3; 

function setup(){
  createCanvas(400,300);
}

function draw() {
  background(255, 254, 237)
  xPos = xPos + xSpeed; 
  yPos = yPos + ySpeed; 
  
  if (mouseX>100) 
    {
  fill(245, 27, 60);
  circle (mouseX, mouseY, 30)
    }
  if (xPos > 100) {
    fill(71, 124, 237);
  }
  circle(mouseX, mouseY, 30);
}
